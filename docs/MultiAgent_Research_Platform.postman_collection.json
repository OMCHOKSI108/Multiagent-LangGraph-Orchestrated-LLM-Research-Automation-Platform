  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set default headers for all requests",
          "pm.request.headers.add({key: 'User-Agent', value: 'PostmanRuntime/7.32.0'});",
          "",
          "// Auto-set Authorization header if JWT token is available",
          "if (pm.environment.get('jwt_token')) {",
          "    pm.request.headers.add({key: 'Authorization', value: 'Bearer ' + pm.environment.get('jwt_token')});",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Auto-save JWT token after login",
          "if (pm.request.url.path.includes('login') && pm.response.code === 200) {",
          "    const response = pm.response.json();",
          "    if (response.token) {",
          "        pm.environment.set('jwt_token', response.token);",
          "        console.log('JWT token saved to environment');",
          "    }",
          "}",
          "",
          "// Auto-save API key after generation",
          "if (pm.request.url.path.includes('apikey') && pm.response.code === 200) {",
          "    const response = pm.response.json();",
          "    if (response.key && response.key.key_value) {",
          "        pm.environment.set('api_key', response.key.key_value);",
          "        console.log('API key saved to environment');",
          "    }",
          "}",
          "",
          "// Auto-save research ID after starting research",
          "if (pm.request.url.path.includes('start') && pm.response.code === 202) {",
          "    const response = pm.response.json();",
          "    if (response.job_id) {",
          "        pm.environment.set('research_id', response.job_id);",
          "        console.log('Research ID saved to environment');",
          "    }",
          "}",
          "",
          "// Auto-save session ID from chat responses",
          "if (pm.request.url.path.includes('message') && pm.response.code === 200) {",
          "    const response = pm.response.json();",
          "    if (response.session_id) {",
          "        pm.environment.set('session_id', response.session_id);",
          "        console.log('Session ID saved to environment');",
          "    }",
          "}"
        ]
      }
    }
  ]
